{
  "name": "skykit-optimizer",
  "version": "1.0.0",
  "description": "SAP Hackathon - Rotables Kit Optimizer",
  "main": "dist/backend/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/index.js",
    "backend": "ts-node src/backend/index.ts",
    "backend:game": "ts-node src/backend/game.ts",
    "frontend": "cd src/frontend && npm run dev",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "watch": "tsc -w"
  },
  "keywords": ["hackathon", "sap", "optimization"],
  "author": "Echipa Hackathon",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
